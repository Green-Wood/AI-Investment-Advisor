page{
  width: 100%;
  height:100%;
  overflow: hidden;
}
/* 知道按钮 */
.btn3{
  margin-top: 10%;
  width: 50%;
  border-radius:98rpx;
  background-color:#0082c8;
  color:white;
  box-shadow: 3px 3px 5px 1px rgb(196, 196, 196);
}

.btn3::after{
  border-radius:98rpx;
  border:0;
}

.start{ /* “知道”按钮文字 */
  font-size:33rpx;
  color: white;
}

/* 返回按钮 */
.btn4{
  margin-top: 5%;
  width: 50%;
  border-radius:98rpx;
  background-color:#ffffff;
  box-shadow: 3px 3px 5px 1px rgb(196, 196, 196);
}

.btn4::after{
  border-radius:98rpx;
  border:0;
}

.skip{
  font-size:33rpx;
  color: rgb(2, 108, 179);
}

