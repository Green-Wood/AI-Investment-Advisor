<!--pages/risk_test/risk_test1.wxml-->
<view class="window">
  <text>
   2、您的家庭可支配年收入为(折合人民币) ： 
  </text>
  
  <view class="choices">
    <button type="setDefault" size="setDefault" loading="{{loading}}" plain="{{plain}}"
            disabled="{{disabled}}" bindtap="jumpToQ3"
            hover-class="button-hover"> A、50万元以下
    </button>

    <button type="setDefault" size="setDefault" loading="{{loading}}" plain="{{plain}}"
            disabled="{{disabled}}" bindtap="jumpToQ3"
            hover-class="button-hover"> B、50—100万元
    </button>

    <button type="setDefault" size="setDefault" loading="{{loading}}" plain="{{plain}}"
            disabled="{{disabled}}" bindtap="jumpToQ3"
            hover-class="button-hover"> C、100—500万元
    </button>

    <button type="setDefault" size="setDefault" loading="{{loading}}" plain="{{plain}}"
            disabled="{{disabled}}" bindtap="jumpToQ3"
            hover-class="button-hover"> D、500—1000万元
    </button>

    <button type="setDefault" size="setDefault" loading="{{loading}}" plain="{{plain}}"
            disabled="{{disabled}}" bindtap="jumpToQ3"
            hover-class="button-hover"> E、1000万元以上
    </button>

  </view>
</view>
