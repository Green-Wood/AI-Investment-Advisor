<!--pages/ask_risk/ask_risl.wxml-->


<view class='content'>
  <view class='content_son'>
    <text> 
    开始风险测试
    </text>
  </view>
</view>

<view>
  <button type="primary" size="setDefault" loading="{{loading}}" plain="{{plain}}"
          disabled="{{disabled}}" bindtap="jumpToRiskTest">
          开始
  </button>

  <button type="setDefault" size="setDefault" loading="{{loading}}" plain="{{plain}}"
          disabled="{{disabled}}" bindtap="skipToRiskChoices">
          跳过
  </button>
</view>
