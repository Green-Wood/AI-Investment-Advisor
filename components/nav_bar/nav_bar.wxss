/* components/nav_bar/nav.bar.wxss */

.nav-wrap {
  display: flex;
  align-items: center;
  width: 100%;
  min-height: 10%;
  max-height: 10%;
  background: #0082c8;
  z-index: 9999999;
}

/* 标题要居中 */
.nav-title {
  position: absolute;
  top: 5.75%;
  bottom: 2%;
  left: 40.5%;
  right: 39.5%;
  width: 20%;
  font-size: 110%;
  color: white;
  font-weight: 600;
}

.nav-capsule {
  display: flex;
  align-items: flex-start;
}

/* 返回键 */
.back-pre{
  position: absolute;
  left: 6%;
  top: 6%;
  bottom: 2%;
  width: 40rpx;
  height: 35rpx;
}

.navbar-v-line {
  position: absolute;
  left: 17%;
  top: 6%;
  bottom: 2%;
  width: 1px;
  height: 35rpx;
  background-color:white;
}

/* Home键 */
.back-home{
  position: absolute;
  left: 24%;
  top: 6%;
  bottom: 2%;
  width: 40rpx;
  height: 35rpx;
}